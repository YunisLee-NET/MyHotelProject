                    </div>
                </div>
            </div>
            <!-- #/ container -->
        </div>

        <partial name="/Views/AdminLayout/FooterPartial.cshtml" />

    </div>

    <script src="~/adminpanel/plugins/common/common.min.js"></script>
    <script src="~/adminpanel/js/custom.min.js"></script>
    <script src="~/adminpanel/js/settings.js"></script>
