@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<partial name="/Views/AdminLayout/HeadPartial.cshtml" />


<body>

    <partial name="/Views/AdminLayout/PreLoaderPartial.cshtml" />

    <div id="main-wrapper">

        <partial name="/Views/AdminLayout/NavHeaderPartial.cshtml" />

        <partial name="/Views/AdminLayout/HeaderPartial.cshtml" />

        <partial name="/Views/AdminLayout/SidebarPartial.cshtml" />

        <div class="content-body">

           
